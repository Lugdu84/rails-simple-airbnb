

<div class="container">
  <h5>Nouvel appartement</h5>
  <%= simple_form_for @flat do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :price_per_night %>
    <%= f.input :number_of_guests %>
    <%= f.submit "Validez", class: "btn btn-primary" %>
  <% end %>
</div>

